{
  "name": "@web-widget/web-server",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && rm -rf tsconfig.tsbuildinfo && pnpm exec tsc && echo 'done'",
    "build:watch": "pnpm exec tsc --watch --incremental"
  },
  "exports": {
    ".": {
      "types": "./dist/server/mod.d.ts",
      "default": "./dist/server/mod.js"
    },
    "./client": {
      "types": "./dist/client/mod.d.ts",
      "default": "./dist/client/mod.js"
    },
    "./runtime/html": {
      "types": "./dist/runtime/html/mod.server.d.ts",
      "browser": "./dist/runtime/html/mod.client.js",
      "default": "./dist/runtime/html/mod.server.js"
    }
  },
  "sideEffects": ["./dist/client/mod.js", "./client.ts"],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "runtime",
    "src",
    "index.ts",
    "client.ts"
  ],
  "dependencies": {
    "@web-widget/web-widget": "workspace:*",
    "@worker-tools/html": "^2.0.0-pre.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "urlpattern-polyfill": "^9.0.0"
  }
}
